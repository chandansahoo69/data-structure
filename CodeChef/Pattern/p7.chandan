#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--)
	{
	    int a,b,c,d;
	    cin>>a>>b>>c>>d;
	    int s=a+b+c;
	    if(s<=d){
	        cout<<1<<endl;
	    }
	    else if((s-c)<=d){
	        cout<<2<<endl;
	    }
	    else{
	        cout<<3<<endl;
	    }
	}
	return 0;
}
